{
  "env": {
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "valid-jsdoc": [
      "error",
      {
        "prefer": {
          "return": "returns"
        }
      }
    ]
  }
}
